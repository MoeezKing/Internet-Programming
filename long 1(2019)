import java.util.Scanner;

public class Mythread extends Thread {
    String name;
    public int num;

    Mythread(String name) {
        super(name);
    }

    @Override
    public void run() {
        for (int i = 0; i <= num; i++) {
            if (isOdd(i))
                System.out.println(i);
        }
    }

    private boolean isOdd(int n) {
        return (n % 2 != 0);
    }
}


